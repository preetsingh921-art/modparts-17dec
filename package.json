{
  "name": "sardaarji-auto-parts",
  "version": "1.0.0",
  "description": "Sardaarji Auto Parts - Full Stack Application",
  "main": "index.js",
  "scripts": {
    "dev": "node dev-server.js",
    "build": "cd frontend && npm run build",
    "vercel-build": "cd frontend && npm install && npm run build",
    "postinstall": "node install-bcryptjs.js || echo 'bcryptjs installation skipped'",
    "prestart": "node fix-bcrypt.js",
    "start": "node index.js",
    "setup-storage": "node scripts/setup-supabase-storage.js",
    "setup-uploads": "node scripts/setup-upload-dirs.js",
    "migrate-images": "node scripts/migrate-images-to-supabase.js",
    "migrate-images:dry-run": "node scripts/migrate-images-to-supabase.js --dry-run",
    "fix-image-urls": "node scripts/fix-nan-image-urls.js",
    "fix-image-urls:comprehensive": "node scripts/fix-nan-image-urls.js --comprehensive",
    "deploy": "node git-deploy.js",
    "deploy:quick": "node git-deploy.js \"Quick update: $(date)\"",
    "deploy:dev": "node git-deploy.js \"Development update\" develop",
    "deploy:prod": "node git-deploy.js \"Production release\" main",
    "sitemap": "node generate-sitemap.js",
    "sitemap:preview": "node generate-sitemap.js --dry-run"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.10.0",
    "bcryptjs": "^2.4.3",
    "busboy": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.0.1",
    "express-session": "^1.18.2",
    "form-data": "^4.0.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.5",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "react-google-recaptcha": "^3.1.0",
    "sharp": "^0.34.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "vite": "^7.0.4"
  }
}
